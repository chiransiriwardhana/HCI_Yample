<script>
    function lowToHigh(){
        var now = window.location.href;
        if(now.includes('name')){
            if(now.includes('order')){
                    now = now.replace('order=high','order=low');
            }
            else{
                now = now+"&"+"order=low";
            }
        } else{
            if(now.includes('order')){
                    now = now.replace('order=high','order=low');
            }
            else{
                now = now+"?"+"order=low";
            }
        }
        window.location.href = now;
    }
    function highToLow(){
        var now = window.location.href;
        if(now.includes('name')){
            if(now.includes('order')){
                now = now.replace('order=low','order=high');
            }
            else{
                now = now+"&"+"order=high";
            }
        } else{
            if(now.includes('order')){
                    now = now.replace('order=low','order=high');
            }
            else{
                now = now+"?"+"order=high";
            }
        }
        window.location.href = now;
    }
    function recentProducts(){
        var now = window.location.href;
        if(now.includes('name')){
            if(now.includes('order')){
                now = now.replace('order=low','order=date');
                now = now.replace('order=high','order=date');
            }
            else{
                now = now+"&"+"order=date";
            }
        } else{
            if(now.includes('order')){
                now = now.replace('order=low','order=date');
                now = now.replace('order=high','order=date');
            }
            else{
                now = now+"?"+"order=date";
            }
        }
        window.location.href = now;
    }
</script>



